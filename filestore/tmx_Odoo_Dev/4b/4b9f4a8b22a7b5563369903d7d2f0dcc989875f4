eadView:one2one('mail.thread_view',{related:'threadViewer.threadView',}),threadViewer:one2one('mail.thread_viewer',{default:[['create']],inverse:'discuss',isCausal:true,}),};Discuss.modelName='mail.discuss';return Discuss;}
